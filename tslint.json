{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        /*"quotemark": [true, "single", "jsx-double"],*/
        "semicolon": [
            true,
            "always"
        ],
        "interface-name": [
            true,
            "always-prefix"
        ],
        "object-literal-sort-keys": false,
        "no-console": false,
        "no-empty": false,
        "no-object-literal-type-assertion": false,
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "space-before-function-paren": false,
        "ordered-imports": false,
        "max-line-length": [
            false
        ],
        "jsx-wrap-multiline": false,
        "jsx-no-lambda": false,
        "prefer-conditional-expression": false,
        "array-type": false
    },
    "rulesDirectory": []
}